<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="br.com.example.grupoestudantildemusica">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
            android:name=".GEMApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".CadastroContatosActivity">
        </activity>
        <activity android:name=".InstrumentosActivity">
        </activity>
        <activity android:name=".OuvidoriaActivity">
        </activity>
        <activity android:name=".ContatoProfessoresActivity">
        </activity>
        <activity android:name=".BibliotecaActivity">
        </activity>
        <activity android:name=".AulasMinistradasActivity">
        </activity>
        <activity android:name=".HorarioActivity">
        </activity>
        <activity android:name=".ManualAlunoActivity">
        </activity>
        <activity
                android:name=".AjudaActivity"
                android:theme="@style/MenuTheme">
        </activity>
        <activity
                android:name=".SobreActivity"
                android:theme="@style/MenuTheme">
        </activity>
        <activity
                android:name=".NotasActivity"
                android:theme="@style/MenuTheme">
        </activity>
        <activity
                android:name=".FaltasActivity"
                android:theme="@style/MenuTheme">
        </activity>
        <activity
                android:name=".CalendarioActivity"
                android:theme="@style/MenuTheme">
        </activity>
        <activity
                android:name=".TelaInicialActivity"
                android:theme="@style/AppTheme.Toolbar">
        </activity>
        <activity
                android:name=".LoginActivity"
                android:theme="@style/LoginTheme">
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>